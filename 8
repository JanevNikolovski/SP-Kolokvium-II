#include <stdio.h>
#define MAX 100
//#define DEBUG 1
void big(int *a)
{
    if (*a<0)
    {
        *a*=-1;
    }
}
int main() {
    int a[MAX][MAX];
    int n,m;
    int top,bot;
    scanf("%d %d\n",&n,&m);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            scanf("%d ",&a[i][j]);
        }
    }
    if (n%2==0)
    {
        for(int j=0;j<m;j++)
        {
            top=0;
            bot=0;
            for (int i=0;i<n;i++)
            {
                if (i < (n/2))
                {
                    top+=a[i][j];

                }
                if (i >= (n / 2))
                {
                    bot+=a[i][j];
                }
            }
#ifdef DEBUG
            printf(" top:%d bot:%d\n",top,bot);
#endif

            a[(n/2)-1][j]=top-bot;
            big(&a[(n/2)-1][j]);
            a[(n/2)][j]=a[(n/2-1)][j];


        }
    }
    else
    {
        for(int j=0;j<m;j++)
        {
            top=0;
            bot=0;
            for (int i=0;i<n;i++)
            {
                if (i <= (n/2))
                {
                    top+=a[i][j];

                }
                if (i >= (n / 2))
                {
                    bot+=a[i][j];
                }
            }
#ifdef DEBUG
            printf(" top:%d bot:%d\n",top,bot);
#endif
            a[(n/2)][j]=top-bot;
            big(&a[(n/2)][j]);
        }
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            printf("%d ",a[i][j]);
        }
        printf("\n");
    }
}
